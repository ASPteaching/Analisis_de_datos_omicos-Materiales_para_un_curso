	volume = "7",
	number = "3",
	pages = "495--501",
	month = apr,
	keywords = "Asparaginase, Data Interpretation, Statistical, Fatigue Syndrome, Chronic, Humans, Models, Statistical, Oligonucleotide Array Sequence Analysis, Predictive Value of Tests, Signal Transduction, Treatment Outcome",
